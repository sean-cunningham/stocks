# Copy to .env and fill in; never commit .env
# LLM provider: stub | xai | openai
LLM_PROVIDER=stub

# API keys (leave blank for stub)
XAI_API_KEY=
OPENAI_API_KEY=

# Model names (placeholders; used when provider is not stub)
XAI_MODEL_CHEAP=
XAI_MODEL_EXPENSIVE=

# Paper trading
PAPER_PORTFOLIO_USD=100000

# CORS (comma-separated origins)
ALLOWED_ORIGINS=http://localhost:3000
